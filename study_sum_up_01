r = f.read()
print(r)

f.close()
f = open("C:\\Users\\houhh\\OneDrive\\바탕 화면\\wow.txt", "a")    # a 는 추가
f.write("brovo~!\n")
f = open("C:\\Users\\houhh\\OneDrive\\바탕 화면\\wow.txt", "r")
r = f.read()
print(r)

f.close()

with open("C:\\Users\\houhh\\OneDrive\\바탕 화면\\wow.txt","r", encoding = "utf-8") as f:    # with open 은 f.close 를 안써도 자동으로 닫히고
    while True:                                                                             # 아래처럼  f.readline()을 쓰게되면 한줄씩 읽어준다.
        read_data = f.readline()
        if not read_data:
            break
        print(read_data)                    # print(read_data, end="")    # 끝에 end=""를 붙이면 바로다음줄 줄간격을 한줄씩 붙여서 가져옴
print(f.closed)                                                # 하지만 붙이지 않았기에 띄어서 가져온다.

# print(f.closed) 를 쓰게되면 파일이 닫힌걸 확인가능하다

print("------------------------------------------------------------------------------------------------------------------------------")
print('\n')


print("자판기 만들기")


i = input("What kind beverage do you want?:\n\n1.Cola 2.Sprite 3.OrangeJuice 4.Water")

Money = int()   # 돈을 미리 입력해놓으면 int로 정수로 변환할 필요 없이 되지만
                # Money = [] 리스트로 해놓을 수도 잇지만 아래처럼 Money 를 정수로 int_money = int(Money)바꿔줘야한다.
cola = 1000
sprite = 1000
orangejuice = 1200
water = 500

if i == "1":
    Money = input("Please press put your money:")
    int_money = int(Money)
    if int_money < cola:
        print("Not enough money.")
    elif int_money > cola:
        print("Your cola is coming, your change is %d won" %(int_money - cola))

### if i == "1":
### money = int(input("enter your money :"))    # 더 간단하게 줄이는 법은 int를 처음부터 씌운다.
### if money < coke:
###     print("not enough money sorry")
### if money > coke:
###     print("your beverage is coming out, thank you. your change is {}".format(money - coke))


if i == "2":
    Money = input("Please press put your money:")
    money = int(Money)
    if money < sprite:
        print("Not enough money.")
    elif money > sprite:
        print("Your sprite is coming, your change is %d won" %(money - sprite))   # 아래도 위와같이 변환해보시오~


if i == "3":
    q = input("The price of orange juice is 1200 because it's made of real fresh orange, are you sure to continue?:")
    if q == "y":
        Money = input("Please put your money in:")
        money = int(Money)
    if money < orangejuice:
        print("돈이 부족합니다.")
    elif money > orangejuice:
        print("Your orangejuice is coming, your change is %d won" %(money - orangejuice))

if i == "4":
    Money = input("Please press put your money:")
    money = int(Money)
    if money < water:
        print("돈이 부족합니다.")
    elif money > water:
        print("Your water is coming, your change is %d won" %(money - water))

print("Thank you and have good day~")

print("------------------------------------------------------------------------------------------------------------------------------")
print('\n')

print("더 효율적인 코딩 작성")  # 더 효율적인 코딩작성

num1 = 100_000_000_000        # 언더바는 계산에 전혀 영향을 미치지 않는다
num2 = 10_000_000_000

total = num1 * num2

print(total)


print("------------------------------------------------------------------------------------------------------------------------------")
print('\n')

print("더 효율적인 코딩 작성")  # 더 효율적인 코딩작성


names = ["bill", "wilcon", "ron", "vilon"]
heroes = ["Spiderman", "Superman", "Deadpool", "Batman"]
universes = ["Marvel", "DC", "Marvel", "DC"]

index = 0
for name in names:              # 이렇게 작성해도 되지만 줄이길고 비효율적이다.
    print(index, name)
    index += 1

print("\n")

for index, name in enumerate(names, start = 1):      # enumerate 를 사용하여 줄여서 효율적인 작성이 가능하다.
    print(index, name)                               # 하지만 위와 다르게 1부터 시작한다.


print("\n")

for index, name in enumerate(names):                # 또다른 비슷한 적용 예시이다.
    hero = heroes[index]
    print(f'{name} is actually {hero}')

print("\n")

for name, hero, universe in zip(names, heroes, universes):    # 바로위 예시를 효율적으로 작성한 코딩이다.
    print(f'{name} is actually {hero} from {universe}')

print("\n")

for value in zip(names, heroes, universes):       # value  를 써서 한꺼번에 더 간단하게 출력할수 있다
    print(value)                                  # 하지만 문장은 생략되고 키와 값만 출력된다.


print("------------------------------------------------------------------------------------------------------------------------------")
print('\n')

# unpacking

a, b = (1, 2)

print(a)
print(b)

print('\n')

a, _ = (1, 2)

print(a)

print('\n')

a, b, *c = (1,2,3,4,5,6,7)   # a와 b 까지는 자리에 맞게 잘 언팩킹이 되지만  c 다음부터는 자리갯수가 맞지않아 에러가난다
                             # 이럴때에는 마지막 c 앞에 * 을붙이면 나머지 초과하는 자릿수의 수자들이 c안으로 다 들어간다.
print(a)
print(b)
print(c)

print('\n')

a, b, *c, d = (1,2,3,4,5,6,7)    # 이렇게 *이 붙는 문자 뒤에 또 문자가 있을경우 d는 마지막 7만을 가져가고 그전까지의
                                 # 숫자만 c 가 다 가져가게 된다.
print(a)
print(b)
print(c)
print(d)
