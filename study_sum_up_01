f = open('File name.txt', 'w')   # 새로 열어서 덮어씀
f.write("안뇽.\n")
f.close()

f = open('File name.txt', 'a') # 파일 추가
f.write('아하로아호아.\n')
f.close()

f = open('File name.txt', 'r') 
data = f.read()
f.close()

f = open('File name.txt', 'rb') 
data = f.read()
f.close()


print(data)   

